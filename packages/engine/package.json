{
  "author": "Gabriel Malosto",
  "dependencies": {},
  "description": "Engine to simulate and create AI models for tabletop games, inspired by the AlphaZero algorithm.",
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "ts-graphviz": "^2.1.5",
    "typescript": "^5.7.3"
  },
  "exports": {
    "./engine/*": {
      "default": "./dist/engine/*",
      "types": "./src/engine/*"
    },
    "./index": {
      "default": "./dist/index.js",
      "types": "./src/index.ts"
    },
    "./interface/*": {
      "default": "./dist/interface/*",
      "types": "./src/interface/*"
    },
    "./types": {
      "default": "./dist/types.js",
      "types": "./src/types.ts"
    }
  },
  "license": "AGPL-3.0-only",
  "name": "@repo/engine",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "pnpm exec prettier . -w",
    "lint": "pnpm exec eslint ./src/** --fix",
    "typecheck": "pnpm exec tsc"
  },
  "type": "module",
  "version": "4.3.0"
}
