{
	"name": "alphazero",
	"version": "1.0.0",
	"description": "Engine to simulate tabletop games, inspired by AlphaZero algorithm",
	"author": "Gabriel Malosto",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "jest"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@types/ink": "^2.0.3",
		"ink": "^4.1.0",
		"ink-select-input": "^5.0.0",
		"ink-text-input": "^5.0.1",
		"meow": "^11.0.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@cspotcode/source-map-support": "^0.8.1",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/ink-testing-library": "^1.0.1",
		"@types/jest": "^29.5.3",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"babel-jest": "^29.6.1",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"jest": "^29.6.1",
		"nodemon": "^3.0.1",
		"prettier": "^2.8.7",
		"rimraf": "^5.0.1",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
